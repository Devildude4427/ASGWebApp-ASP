@{
    ViewData["Title"] = "ASG Dashboard";
}

@section Header {
    @Html.Partial("_Header")
}
                
@section Sidebar {
    @Html.Partial("_Sidebar")
}
                
<div class="page-wrapper">
    <div class="page-breadcrumb">
        <div class="row">
            <div class="col-12 d-flex align-items-center">
                <h4 class="page-title">Dashboard</h4>
                <div class="ml-auto text-right">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a asp-controller="Dashboard" asp-action="Index">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Library</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
                    
    </div>
    <div class="container-fluid">
        <div class="row">
            <!-- Column -->
            <div class="col-md-6 col-lg-2" id="courseRegistrationCard">
                <div class="card card-hover">
                    <a asp-controller="Dashboard" asp-action="Course">
                        <div class="box bg-cyan text-center">
                            <h1 class="font-light text-white"><i class="mdi mdi-receipt"></i></h1>
                            <h6 class="text-white">Course Registration</h6>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("_Footer")
</div>
            
@section Scripts {
    <script src="~/vendors/flot/excanvas.js"></script>
    <script src="~/vendors/flot/jquery.flot.js"></script>
    <script src="~/vendors/flot/jquery.flot.pie.js"></script>
    <script src="~/vendors/flot/jquery.flot.time.js"></script>
    <script src="~/vendors/flot/jquery.flot.stack.js"></script>
    <script src="~/vendors/flot/jquery.flot.crosshair.js"></script>
    <script src="~/vendors/flot.tooltip/js/jquery.flot.tooltip.min.js"></script>
    <script src="~/js/pages/chart/chart-page-init.js"></script>
            
    <script>
        $.ajax({
            type: "GET",
            url: "/api/v1/user",
            success: function(result)
            {
                const response = JSON.parse(JSON.stringify(result));
                console.log(response);
            },
            failure: function() {
                alert("Failed to get user data");
            }
        });
    </script>
}